<?xml version="1.0" encoding="UTF-8"?>
<!--
@version 3.2.0.1
-->
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

    <bean name="travelRouteIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="strategy"   ref="travelRouteMstrStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="JNITSEQ"/>
        <property name="tableName"  value="TRAVEL_ROUTE_ID"/>
    </bean>
    <bean name="travelRouteMstrStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="ROUT" />
        <property name="cipers"   value="6" /> 
        <property name="fillChar" value="0" />
    </bean>
     
    <bean name="travelRouteGroupIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="strategy"   ref="travelRouteGroupMstrStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="JNITSEQ"/>
        <property name="tableName"  value="TRAVEL_ROUTE_GROUP"/>
    </bean>
    <bean name="travelRouteGroupMstrStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="RTGRP" />
        <property name="cipers"   value="5" /> 
        <property name="fillChar" value="0" />
    </bean>
     
    <bean name="travelDestIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="strategy"   ref="travelDestMstrStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="JNITSEQ"/>
        <property name="tableName"  value="TRAVEL_DEST_ID"/>
    </bean>
    <bean name="travelDestMstrStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="DEST" />
        <property name="cipers"   value="6" /> 
        <property name="fillChar" value="0" />
    </bean>
     
    <bean name="travelFameHisIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="strategy"   ref="travelFameHisMstrStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="JNITSEQ"/>
        <property name="tableName"  value="TRAVEL_FAME_HIS_ID"/>
    </bean>
    <bean name="travelFameHisMstrStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="FMHS" />
        <property name="cipers"   value="6" /> 
        <property name="fillChar" value="0" />
    </bean>
     
    <bean name="travelClipIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="strategy"   ref="travelClipMstrStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="JNITSEQ"/>
        <property name="tableName"  value="TRAVEL_CLIP_ID"/>
    </bean>
    <bean name="travelClipMstrStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="CLP" />
        <property name="cipers"   value="7" /> 
        <property name="fillChar" value="0" />
    </bean>
     
</beans>
