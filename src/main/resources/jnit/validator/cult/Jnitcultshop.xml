<!DOCTYPE form-validation PUBLIC 
    "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" 
    "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>

    <formset>
		<form name="jnitcultshopVO">
			<field property="shopNm" depends="required">
        		<arg0 key="쇼핑 명칭" />
			</field>
			
			<field property="areaId" depends="required">
        		<arg0 key="지역 정보" />
			</field>
			
			<field property="zoneId" depends="required">
        		<arg0 key="권역 정보" />
			</field>
			
			<field property="originId" depends="required">
        		<arg0 key="출처 정보" />
			</field>
			
			<field property="shopEmail" depends="email">
        		<arg0 key="이메일" />
			</field>
		</form>
    </formset>

</form-validation>